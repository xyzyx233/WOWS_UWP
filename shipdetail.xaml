        <TextBlock x:Name="textBlock_Copy6" HorizontalAlignment="Left" Height="28" Margin="10,287,0,0" TextWrapping="Wrap" Text="舰艇权重：" VerticalAlignment="Top" Width="113" FontSize="22" TextAlignment="Center"/>
        <TextBlock x:Name="textBlock_Copy7" HorizontalAlignment="Left" Height="28" Margin="10,320,0,0" TextWrapping="Wrap" Text="基础权重：" VerticalAlignment="Top" Width="113" FontSize="22" TextAlignment="Center"/>
        <TextBlock x:Name="textBlock_Copy8" HorizontalAlignment="Left" Height="28" Margin="10,353,0,0" TextWrapping="Wrap" Text="国服名称：" VerticalAlignment="Top" Width="113" FontSize="22" TextAlignment="Center"/>
        <TextBlock x:Name="textBlock_Copy9" HorizontalAlignment="Left" Height="28" Margin="10,386,0,0" TextWrapping="Wrap" Text="是否存在：" VerticalAlignment="Top" Width="113" FontSize="22" TextAlignment="Center"/>
        <TextBlock x:Name="cd" HorizontalAlignment="Left" Height="28" Margin="128,56,0,0" TextWrapping="Wrap" Text=" " VerticalAlignment="Top" Width="222"/>
        <TextBlock x:Name="id" HorizontalAlignment="Left" Height="28" Margin="138,89,0,0" TextWrapping="Wrap" Text=" " VerticalAlignment="Top" Width="212"/>
        <TextBlock x:Name="name" HorizontalAlignment="Left" Height="28" Margin="138,122,0,0" TextWrapping="Wrap" Text=" " VerticalAlignment="Top" Width="212"/>
        <TextBlock x:Name="alias" HorizontalAlignment="Left" Height="28" Margin="138,155,0,0" TextWrapping="Wrap" Text=" " VerticalAlignment="Top" Width="212"/>
        <TextBlock x:Name="country" HorizontalAlignment="Left" Height="28" Margin="138,188,0,0" TextWrapping="Wrap" Text=" " VerticalAlignment="Top" Width="212"/>
        <TextBlock x:Name="tyoe" HorizontalAlignment="Left" Height="28" Margin="138,221,0,0" TextWrapping="Wrap" Text=" " VerticalAlignment="Top" Width="212"/>
        <TextBlock x:Name="lvl" HorizontalAlignment="Left" Height="28" Margin="138,254,0,0" TextWrapping="Wrap" Text=" " VerticalAlignment="Top" Width="212"/>
        <TextBlock x:Name="weight" HorizontalAlignment="Left" Height="28" Margin="138,287,0,0" TextWrapping="Wrap" Text=" " VerticalAlignment="Top" Width="212"/>
        <TextBlock x:Name="baseweight" HorizontalAlignment="Left" Height="28" Margin="148,320,0,0" TextWrapping="Wrap" Text=" " VerticalAlignment="Top" Width="212"/>
        <TextBlock x:Name="cnname" HorizontalAlignment="Left" Height="28" Margin="138,353,0,0" TextWrapping="Wrap" Text=" " VerticalAlignment="Top" Width="212"/>
        <TextBlock x:Name="jump" HorizontalAlignment="Left" Height="28" Margin="128,386,0,0" TextWrapping="Wrap" Text=" " VerticalAlignment="Top" Width="212"/>
        <TextBlock x:Name="backtoparents" HorizontalAlignment="Left" Height="66" Margin="10,461,0,0" TextWrapping="Wrap" Text="返回" VerticalAlignment="Top" Width="340" TextAlignment="Center" FontSize="48" Tapped="backtoparents_Tapped"/>
    </Grid>
</Page>
