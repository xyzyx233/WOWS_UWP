<Page
    x:Class="WOWS_UWP.shipdetail"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:WOWS_UWP"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <FlyoutBase.AttachedFlyout>
            <Flyout/>
        </FlyoutBase.AttachedFlyout>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Height="28" Margin="10,56,0,0" TextWrapping="Wrap" Text="舰艇代码：" VerticalAlignment="Top" Width="113" FontSize="22" TextAlignment="Center"/>
        <TextBlock x:Name="textBlock_Copy" HorizontalAlignment="Left" Height="28" Margin="10,89,0,0" TextWrapping="Wrap" Text="舰艇编号：" VerticalAlignment="Top" Width="113" FontSize="22" TextAlignment="Center"/>
        <TextBlock x:Name="textBlock_Copy1" HorizontalAlignment="Left" Height="28" Margin="10,122,0,0" TextWrapping="Wrap" Text="英文名：" VerticalAlignment="Top" Width="113" FontSize="22" TextAlignment="Center"/>
        <TextBlock x:Name="textBlock_Copy2" HorizontalAlignment="Left" Height="28" Margin="10,155,0,0" TextWrapping="Wrap" Text="中文名：" VerticalAlignment="Top" Width="113" FontSize="22" TextAlignment="Center"/>
        <TextBlock x:Name="textBlock_Copy3" HorizontalAlignment="Left" Height="28" Margin="10,188,0,0" TextWrapping="Wrap" Text="所属国家：" VerticalAlignment="Top" Width="113" FontSize="22" TextAlignment="Center"/>
        <TextBlock x:Name="textBlock_Copy4" HorizontalAlignment="Left" Height="28" Margin="10,221,0,0" TextWrapping="Wrap" Text="舰艇类型：" VerticalAlignment="Top" Width="113" FontSize="22" TextAlignment="Center"/>
